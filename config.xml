<?xml version='1.0' encoding='utf-8'?>
<widget id="de.mission-mobile.demo.mm.phonegap" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>FotoApp</name>
    <description>
		FotoApp cordova Test         
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" subdomains="true"/>

<icon src="icon.png" />
<icon src="icons/ios/icon.png" platform="ios" width="57" height="57" />
<icon src="icons/ios/icon-72.png" platform="ios" width="72" height="72" />
<icon src="icons/ios/icon_at_2x.png" platform="ios" width="114" height="114" />
<icon src="icons/ios/icon-72_at_2x.png" platform="ios" width="144" height="144" />
<splash src="splash.png" />
<splash src="splash/ios/Retina.png" platform="ios" width="640" height="960" />
<splash src="splash/ios/Classic.png" platform="ios" width="320" height="480" />
<splash src="splash/ios/iPhone5.png" platform="ios" width="640" height="1136" />
<preference name="phonegap-version" value="3.6.3" />
<preference name="fullscreen" value="true" />
<preference name="StatusBarOverlaysWebView" value="false" />
<preference name="splash-screen-duration" value="10000" />
<preference name="auto-hide-splash-screen" value="false" />

</widget>
